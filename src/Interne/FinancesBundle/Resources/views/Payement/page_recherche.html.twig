{% extends "AppBundle:Layout:layout.html.twig" %}


{% block titre %}Recherche de payement{% endblock %}

{% block content %}

    <div class="ui page grid">
        <div class="row">
            <div class="four wide column">

                {% include "InterneFinancesBundle:Payement:searchForm.html.twig" with {'searchForm':searchForm} %}


            </div>


            <div class="twelve wide column">




                {% if payements is not empty %}



                    {% include "InterneFinancesBundle:Payement:payementsListe.html.twig" with {'results':payements} %}


                {% endif %}



            </div>
        </div>
    </div>


{% endblock %}

{% block js %}


    {% javascripts
    '@InterneFinancesBundle/Resources/public/js/model/*'
    '@InterneFinancesBundle/Resources/public/js/event_catcher.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}


{% endblock %}