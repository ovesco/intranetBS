<h4>Contact</h4>


{% include "AppBundle:Adresse:show_with_form.html.twig" with {'adresse':contact.adresse} %}



{% if contact.emails|length > 0 %}

    <h5>Emails</h5>

    {% for email in contact.emails%}

        {% include "AppBundle:Email:show_with_form.html.twig" with {'email':email} %}

    {% endfor %}

{% else %}
    <div class="ui message yellow">Auncune adresse Email</div>
{% endif %}